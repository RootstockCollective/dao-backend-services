{
    "executionRoleArn": "arn:aws:iam::094889878379:role/ecsTaskExecutionRole",
    "family": "rif-wallet-services-dev",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [ 
        "FARGATE" 
     ],
    "cpu": "256",
    "memory": "1024",
    "containerDefinitions": [
        {
            "name": "rif-wallet-services",
            "image": "094889878379.dkr.ecr.us-west-2.amazonaws.com/rif-wallet-services-dev:latest",

            "portMappings": [
                {
                  "containerPort": 3000
                }
              ],
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-region": "us-west-2",
                "awslogs-group": "rif-wallet-services-dev",
                "awslogs-stream-prefix": "rif-wallet"
              }
            }
          }
    ]
  }
  